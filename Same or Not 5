#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a,i;
    scanf("%d",&a);
     int arr[a];
    for(int i=0;i<a;i++)
        scanf("%d\n",&arr[i]);
    int oddcount=0;
    int evencount=0;
    for(int i=0;i<a;i++)
    {
        if(arr[i]%2==0)
        {
         evencount++;
        }
        else
        {
            oddcount++;
        }
    }
    if(evencount==a)
    {
        printf("Even");
    }
    else if(oddcount==a) 
    {
        printf("Odd");
    }
    else
    {
        printf("Mixed");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
