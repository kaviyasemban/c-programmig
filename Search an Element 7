#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int num,a;
    float evencount=0;
    scanf("%d\n",&num);
    int arr[num];
    for(int i=0;i<num;i++)
    {
      scanf("%d\n",&arr[i]);  
    }
    scanf("%d",&a);
    for(int i=0;i<num;i++)
    {
    
    if(arr[i]==a)
    {
        evencount=1;
        break;
    }
    }
    if(evencount==1)
    {
                  printf("%d is presented in an array.",a);
  
    }
    else
    {
        printf("%d is not presented in an array.",a);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
