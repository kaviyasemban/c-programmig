#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name[50];
    int arrer,cgpa;
    scanf("%s\n",name);
    printf("Name of the Student:%s\n",name);
    scanf("%d\n",&arrer);
    scanf("%d\n",&cgpa);
    if(arrer==1&&cgpa>=70)
    {
        printf("%s is Eligible for Placement",name);
    }
    else if(arrer==2&&cgpa>=75)
    {
        printf("%s is Eligible for Placement",name);
    }
    else
    {
        printf("%s is Not Eligible for Placement",name);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
