#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int num,sum=0;
    scanf("%d\n",&num);
    int arr[num];
    for(int i=0;i<num;i++)
    {
        scanf("%d",&arr[i]);
        sum=sum+arr[i];
    }
        float div=(float)sum/num;
              printf("Array Mean Value is %.2f",div);
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
