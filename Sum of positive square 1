#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int num,sum=0;
   scanf("%d",&num);
    int arr[num];
     
    for(int i=0;i<num;i++)
    {
        scanf("%d\n",&arr[i]);
    }
    for(int i=0;i<num;i++)
    {
    if(arr[i]>0)
    {
      sum=sum+(arr[i]*arr[i]);
     
  }
    }
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
