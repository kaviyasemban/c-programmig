#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a,count=0;
    scanf("%d",&a);
    if((a>=1 && a<=10000000))
    {
            
       int temp=a;
         while(temp>0)
         {
        temp=temp/10;
        count++;
         }
        printf("The count of the given integer is: %d",count);
    }
    else
    {
        printf("Enter a Valid Input");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
